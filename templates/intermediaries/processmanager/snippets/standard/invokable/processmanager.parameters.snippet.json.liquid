{%- capture workflow_object_name_unique -%}{{ workflow_object.name | replace: " ", "_" | replace: ".", "_" | replace: "'", "_" }}_({{ workflow_object.properties.UniqueId }}){%- endcapture -%}
{%- capture workflow_object_name -%}{{ workflow_object.name | replace: " ", "_" | replace: ".", "_" | replace: "'", "_" }}{%- endcapture -%}
{
	"{{ workflow_object_name }}.processManager.scenarioName": {
		"type": "String",
		"value": "{{ resource_template.parameters.scenario_name }}",
		"description": "The name of the scenario for this specific Process Manager."
	},
	"{{ workflow_object_name }}.processManager.scenarioStep": {
		"type": "String",
		"value": "{{ resource_template.parameters.scenario_step_name }}",
		"description": "The name of the scenario step for this specific Process Manager."
	},
	"{{ resource_template.parameters.workflow_name }}.processManager.suspendQueueTopicName": {
		"type": "String",
		"value": "{{ resource_template.parameters.azure_service_bus_suspend_queue_topic_name }}",
		"description": "The name of the topic used for suspended messages."
	}
}