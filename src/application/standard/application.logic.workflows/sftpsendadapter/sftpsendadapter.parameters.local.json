{
	"{{ resource_template.parameters.workflow_name }}.sftpSendConnection.apiId": {
		"type": "String",
		"value": "/subscriptions/@appsetting('apiConnectionSubscriptionId')/providers/Microsoft.Web/locations/@appsetting('apiConnectionLocation')/managedApis/sftpwithssh"
	},
	"{{ resource_template.parameters.workflow_name }}.sftpSendConnection.resourceId": {
		"type": "String",
		"value": "/subscriptions/@appsetting('apiConnectionSubscriptionId')/resourceGroups/@appsetting('apiConnectionResourceGroupName')/providers/Microsoft.Web/connections/@appsetting('{{ resource_template.parameters.workflow_name }}.sftpSendConnection.name')"
	},
	"{{ resource_template.parameters.workflow_name }}.sftpSendConnection.apiAuthentication": {
		"type": "Object",
		"value": {
			"type": "Raw",
			"scheme": "Key",
			"parameter": "@appsetting('{{ resource_template.parameters.workflow_name }}.sftpSendConnection.key')"
		}
	},
	"{{ resource_template.parameters.workflow_name }}.sftpSendAdapter.sftpSendFileName": {
		"type": "String",
		"value": "{{ resource_template.parameters.azure_sftp_target_file_name }}",
		"description": "The name of the file to send (can include macro values e.g. %MessageID% or %SourceFileName%)."
	},
	"{{ resource_template.parameters.workflow_name }}.sftpSendAdapter.sftpSendFolder": {
		"type": "String",
		"value": "{{ resource_template.parameters.azure_sftp_target_folder }}",
		"description": "The path where the FTP connector sends files."
	}
}